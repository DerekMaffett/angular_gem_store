(function() {
  var app = angular.module('GemStore');

  app.config(['$routeProvider', function($routeProvider) {
    $routeProvider
      .when('/products', {
        controller: 'StoreController',
        templateUrl: '<%= asset_path("gem_store_app/products/views/store.html") %>'
      })
      .when('/admins', {
        controller: 'AdminsController',
        templateUrl: '<%= asset_path("gem_store_app/admins/views/admins.html") %>'
      })
      .otherwise({
        redirectTo: '/products'
      });
  }]);
})();
